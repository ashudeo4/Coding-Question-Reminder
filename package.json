{
  "name": "reminder",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production NODE_OPTIONS=--openssl-legacy-provide node app.js",
    "server": "nodemon app.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.6",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^9.0.2",
    "express": "^4.18.2",
    "google-auth-library": "^7.0.4",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^5.12.3",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "concurrently": "^6.0.1",
    "nodemon": "^3.0.1"
  }
}
